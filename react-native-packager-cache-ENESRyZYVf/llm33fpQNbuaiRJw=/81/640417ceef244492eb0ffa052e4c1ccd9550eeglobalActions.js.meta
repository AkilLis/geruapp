["e2f9c6862df343ccde79595f426f14d630805a94","285cbeb9fb9cd451b69cca2d05f8dc560798beda",["../auth/authConstants"],[241],{"version":3,"sources":["/var/www/geruapp/src/global/globalActions.js"],"names":["setSessionToken","setStore","setState","getState","sessionToken","type","payload","store","newState","toggle"],"mappings":";AAKA;;;;;QAkBgBA,e,GAAAA,e;QAcAC,Q,GAAAA,Q;QAUAC,Q,GAAAA,Q;QAUAC,Q,GAAAA,Q;;AA7ChB;;AAWO,SAASH,eAAT,CAA0BI,YAA1B,EAAwC;AAC7C,SAAO;AACLC,0CADK;AAELC,aAASF;AAFJ,GAAP;AAID;AASM,SAASH,QAAT,CAAmBM,KAAnB,EAA0B;AAC/B,SAAO;AACLF,kCADK;AAELC,aAASC;AAFJ,GAAP;AAID;AAKM,SAASL,QAAT,CAAmBM,QAAnB,EAA6B;AAClC,SAAO;AACLH,kCADK;AAELC,aAASE;AAFJ,GAAP;AAID;AAKM,SAASL,QAAT,CAAmBM,MAAnB,EAA2B;AAChC,SAAO;AACLJ,kCADK;AAELC,aAASG;AAFJ,GAAP;AAID","sourcesContent":["/**\n * # globalActions.js\n *\n * Actions that are global in nature\n */\n'use strict'\n\n/**\n * ## Imports\n *\n * The actions supported\n */\nimport {\n  SET_SESSION_TOKEN,\n  SET_STORE,\n  SET_STATE,\n  GET_STATE\n} from '../auth/authConstants'\n\n/**\n * ## set the sessionToken\n *\n */\nexport function setSessionToken (sessionToken) {\n  return {\n    type: SET_SESSION_TOKEN,\n    payload: sessionToken\n  }\n}\n/**\n * ## set the store\n *\n * this is the Redux store\n *\n * this is here to support Hot Loading\n *\n */\nexport function setStore (store) {\n  return {\n    type: SET_STORE,\n    payload: store\n  }\n}\n/**\n * ## set state\n *\n */\nexport function setState (newState) {\n  return {\n    type: SET_STATE,\n    payload: newState\n  }\n}\n/**\n * ## getState\n *\n */\nexport function getState (toggle) {\n  return {\n    type: GET_STATE,\n    payload: toggle\n  }\n}\n"]}]